#!/usr/bin/env python

import urllib2

# Request GB tile from tilecache.
response = urllib2.urlopen('http://dm-maps-kb.edina.ac.uk:9901/os_cache/tilecache.py?REQUEST=GetMap&VERSION=1.1.1&BBOX=1248000%2C524000%2C1760000%2C1036000&WIDTH=256&HEIGHT=256&FORMAT=image%2Fpng&SRS=EPSG%3A27700&PRODUCT=gb&LAYERS=roam_gb')

if response.getcode() == 200:
    print 'Image retrieved'
elif
    print 'Image not retrieved'

       
